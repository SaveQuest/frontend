{
    "semi": true,              // 세미콜론 사용 여부
    "singleQuote": true,       // 작은 따옴표 사용 여부
    "trailingComma": "es5",    // 후행 쉼표 설정 (es5: 객체와 배열에만, all: 모든 곳에)
    "tabWidth": 2,             // 탭 폭 설정
    "useTabs": false,          // 탭 대신 스페이스 사용 여부
    "printWidth": 80,          // 한 줄 최대 길이
    "bracketSpacing": true,     // 객체 리터럴의 괄호 사이 공백 여부
    "jsxBracketSameLine": false // JSX에서 닫는 괄호를 다음 줄로 이동할지 여부
  }
  